<?xml version="1.0" encoding="utf-8"?>
<s:Group xmlns:fx="http://ns.adobe.com/mxml/2009" 
		 xmlns:s="library://ns.adobe.com/flex/spark" 
		 width="{550/(320/Capabilities.screenDPI)}" 
		 height="{100/(320/Capabilities.screenDPI)}" 
		 mouseOver="gOver(event);" mouseDown="gDown(event);"
		 mouseOut="gOut(event);" mouseUp="gOut(event);" >
	<fx:Script source="../func/global.as"/>
	<fx:Script>
		<![CDATA[
			[Bindable]
			public var textLabel:String = "";
	]]>
	</fx:Script>
	<s:Rect width="100%"  height="100%"
			radiusX="{15/(320/Capabilities.screenDPI)}"
			radiusY="{15/(320/Capabilities.screenDPI)}" >
		<s:fill>
			<s:SolidColor alpha="0.6" color="#36ccba"/>
		</s:fill>
	</s:Rect>
	<s:Label color="#FFFFFF" fontWeight="bold" 
			 horizontalCenter="0" styleName="textsize0"
			 text="{textLabel}" textAlign="center" verticalCenter="0"/>
</s:Group>
