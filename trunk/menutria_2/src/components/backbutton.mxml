	xmlns:s="library://ns.adobe.com/flex/spark" >
	<fx:Script>
		<![CDATA[
			import spark.core.ContentCache;
			static public const s_imageCache:ContentCache = new ContentCache();
			
		]]>
	</fx:Script>
	<s:Image verticalCenter="0"  contentLoader="{s_imageCache}"   horizontalCenter="0" mouseEnabled="false">
		<s:source>
			<s:MultiDPIBitmapSource source160dpi="@Embed('assets/160/back.png')"
									source240dpi="@Embed('assets/240/back.png')"
									source320dpi="@Embed('assets/240/back.png')"
									source480dpi="@Embed('assets/240/back.png')"
									source640dpi="@Embed('assets/240/back.png')"
									/>
		</s:source>
	</s:Image>
</s:Group>