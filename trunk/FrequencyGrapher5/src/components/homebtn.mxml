<?xml version="1.0" encoding="utf-8"?>
<s:Skin xmlns:s="library://ns.adobe.com/flex/spark"
		xmlns:fx="http://ns.adobe.com/mxml/2009"
		xmlns:d="http://ns.adobe.com/fxg/2008/dt"
		width="39" height="38">
	<fx:Metadata>[HostComponent("spark.components.Button")]</fx:Metadata>
	<s:states>
		<s:State name="up"/>
		<s:State name="over"/>
		<s:State name="down"/>
		<s:State name="disabled"/>
	</s:states>
	<s:BitmapImage d:userLabel="Layer 1" x="0" y="0" width="39" height="38" smooth="true"
				   source="@Embed('/assets/images/homebtn/Layer 1.png')">
		<s:filters>
			<s:GlowFilter includeIn="over" alpha="1.0" blurX="6.0" blurY="6.0" color="#5380d0"
						  inner="false" knockout="false" quality="2" strength="1"/>
			<s:GlowFilter includeIn="down" alpha="1.0" blurX="6.0" blurY="6.0" color="#000000"
						  inner="false" knockout="false" quality="2" strength="1"/>
		</s:filters>
	</s:BitmapImage>
</s:Skin>
