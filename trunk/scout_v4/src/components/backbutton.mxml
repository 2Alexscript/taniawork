			import spark.core.ContentCache;
			static public const s_imageCache:ContentCache = new ContentCache();
			
		]]>
	</fx:Script>
	<s:Image  left="{20/(320/Capabilities.screenDPI)}" contentLoader="{s_imageCache}"   verticalCenter="0" mouseEnabled="false">
