									source240dpi="@Embed('assets/320/arrow.png')"
									source320dpi="@Embed('assets/320/arrow.png')"
									source480dpi="@Embed('assets/320/arrow.png')"
									source640dpi="@Embed('assets/320/arrow.png')"
									/>
		</s:source>
	</s:Image>
</s:Group>